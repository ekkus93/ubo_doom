# Example environment for running ubo_app with external services
export UBO_SERVICES_PATH="$HOME/ubo_services"

# Doom service config
export UBO_DOOM_LIB="$HOME/doom/libubodoom.so"
export UBO_DOOM_IWAD="$HOME/doom/doom2.wad"
export UBO_DOOM_FPS="30"
# Optional: force ALSA playback PCM device for Doom (e.g. "default", "plughw:0,0").
export UBO_DOOM_ALSA_DEVICE="default"
